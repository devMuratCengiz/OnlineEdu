@model CreateTeacherSocialDto
@{
    ViewData["Title"] = "Create Teacher Social Media";
    Layout = "~/Areas/Teacher/Views/TeacherLayout/Index.cshtml";
}

<form method="post">

    <label>Name</label>
    <input type="text" asp-for="Name" class="form-control" />
    <br />

    <label>Icon</label>
    <input type="text" asp-for="Icon" class="form-control" />
    <br />

    <label>Url</label>
    <input type="text" asp-for="Url" class="form-control" />
    <br />

    <button class="btn btn-outline-primary">Add</button>

</form>
