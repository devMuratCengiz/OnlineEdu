@model CreateCourseRegisterDto
@{
    ViewData["Title"] = "RegisterCourse";
    Layout = "~/Areas/Student/Views/StudentLayout/Index.cshtml";
}

<form method="post">

    <label>Course</label>
    <select asp-for="CourseId" asp-items="@ViewBag.courses" class="form-control">
        <option>Courses</option>
    </select>
    <br />

    <button class="btn btn-outline-primary">Register</button>

</form>